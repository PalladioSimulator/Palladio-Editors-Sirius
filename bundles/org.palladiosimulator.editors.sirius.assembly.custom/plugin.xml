<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension
         point="org.eclipse.sirius.externalJavaAction">
      <javaActions
            actionClass="org.palladiosimulator.editors.sirius.assembly.custom.externaljavaactions.AddAssemblyContext"
            id="AddAssemblyContext">
      </javaActions>
      <javaActions
            actionClass="org.palladiosimulator.editors.sirius.assembly.custom.externaljavaactions.AddOperationProvidedRole"
            id="AddOperationProvidedRole">
      </javaActions>
      <javaActions
            actionClass="org.palladiosimulator.editors.sirius.assembly.custom.externaljavaactions.AddOperationRequiredRole"
            id="AddOperationRequiredRole">
      </javaActions>
      <javaActions
            actionClass="org.palladiosimulator.editors.sirius.assembly.custom.externaljavaactions.AddInfrastructureProvidedRole"
            id="AddInfrastructureProvidedRole">
      </javaActions>
      <javaActions
            actionClass="org.palladiosimulator.editors.sirius.assembly.custom.externaljavaactions.AddInfrastructureRequiredRole"
            id="AddInfrastructureRequiredRole">
      </javaActions>
      <javaActions
            actionClass="org.palladiosimulator.editors.sirius.assembly.custom.externaljavaactions.AddSinkRole"
            id="AddSinkRole">
      </javaActions>
      <javaActions
            actionClass="org.palladiosimulator.editors.sirius.assembly.custom.externaljavaactions.AddSourceRole"
            id="AddSourceRole">
      </javaActions>
      <javaActions
            actionClass="org.palladiosimulator.editors.sirius.assembly.custom.externaljavaactions.DeleteAsemblyContext"
            id="DeleteAsemblyContext">
      </javaActions>
      <javaActions
            actionClass="org.palladiosimulator.editors.sirius.assembly.custom.externaljavaactions.AddSystemSpecifiedExecutionTime"
            id="AddSystemSpecifiedExecutionTime">
      </javaActions>
      <javaActions
            actionClass="org.palladiosimulator.editors.sirius.assembly.custom.externaljavaactions.AddEventChannel"
            id="AddEventChannel">
      </javaActions>
   </extension>
     <extension
        point="org.eclipse.ui.newWizards">
     <wizard
           category="org.palladiosimulator.pcmbench.palladio_modeling_category"
           class="org.palladiosimulator.editors.sirius.assembly.custom.wizard.SystemCreationWizard"
           icon="icons/NewCompositeFile.gif"
           id="org.palladiosimulator.editors.sirius.assembly.custom.wizard.SystemCreationWizard"
           name="System Model and Diagram - Sirius">
     </wizard>
  </extension>
  <extension
        point="org.eclipse.ui.commands">
     <command
           defaultHandler="org.palladiosimulator.editors.sirius.assembly.custom.toolbar.SystemCreation"
           id="org.palladiosimulator.editors.sirius.assembly.custom.toolbar.SystemCreation"
           name="SystemCreation">
     </command>
  </extension>
  <extension
        point="org.eclipse.ui.menus">
     <menuContribution
           allPopups="false"
           locationURI="toolbar:org.palladiosimulator.editors.sirius.ui.PCMToolbar?after=Repository">
        <command
              commandId="org.palladiosimulator.editors.sirius.assembly.custom.toolbar.SystemCreation"
              icon="icons/NewCompositeFile.gif"
              id="System"
              label="Create a new System Model and Diagram from scratch"
              style="push"
              tooltip="Create a new System Model and Diagram from scratch">
        </command>
     </menuContribution>
  </extension>
  <extension
        id="org.palladiosimulator.editors.sirius.assembly.custom.operationProvidedRole"
        point="org.palladiosimulator.editors.sirius.custom.style.rotatableNode">
     <image
           bottom="/org.palladiosimulator.editors.sirius.assembly.custom/images/operation-provided-bottom.svg"
           id="org.palladiosimulator.editors.sirius.assembly.custom.operationProvidedRole"
           left="/org.palladiosimulator.editors.sirius.assembly.custom/images/operation-provided-left.svg"
           right="/org.palladiosimulator.editors.sirius.assembly.custom/images/operation-provided-right.svg"
           top="/org.palladiosimulator.editors.sirius.assembly.custom/images/operation-provided-top.svg">
     </image>
  </extension>
  <extension
        id="org.palladiosimulator.editors.sirius.assembly.custom.operationRequiredRole"
        point="org.palladiosimulator.editors.sirius.custom.style.rotatableNode">
     <image
           bottom="/org.palladiosimulator.editors.sirius.assembly.custom/images/operation-required-bottom.svg"
           id="org.palladiosimulator.editors.sirius.assembly.custom.operationRequiredRole"
           left="/org.palladiosimulator.editors.sirius.assembly.custom/images/operation-required-left.svg"
           right="/org.palladiosimulator.editors.sirius.assembly.custom/images/operation-required-right.svg"
           top="/org.palladiosimulator.editors.sirius.assembly.custom/images/operation-required-top.svg">
     </image>
  </extension>
  <extension
        id="org.palladiosimulator.editors.sirius.assembly.custom.infrastructureProvidedRole"
        point="org.palladiosimulator.editors.sirius.custom.style.rotatableNode">
     <image
           bottom="/org.palladiosimulator.editors.sirius.assembly.custom/images/infrastructure-provided-bottom.svg"
           id="org.palladiosimulator.editors.sirius.assembly.custom.infrastructureProvidedRole"
           left="/org.palladiosimulator.editors.sirius.assembly.custom/images/infrastructure-provided-left.svg"
           right="/org.palladiosimulator.editors.sirius.assembly.custom/images/infrastructure-provided-right.svg"
           top="/org.palladiosimulator.editors.sirius.assembly.custom/images/infrastructure-provided-top.svg">
     </image>
  </extension>
  <extension
        id="org.palladiosimulator.editors.sirius.assembly.custom.infrastructureRequiredRole"
        point="org.palladiosimulator.editors.sirius.custom.style.rotatableNode">
     <image
           bottom="/org.palladiosimulator.editors.sirius.assembly.custom/images/infrastructure-required-bottom.svg"
           id="org.palladiosimulator.editors.sirius.assembly.custom.infrastructureRequiredRole"
           left="/org.palladiosimulator.editors.sirius.assembly.custom/images/infrastructure-required-left.svg"
           right="/org.palladiosimulator.editors.sirius.assembly.custom/images/infrastructure-required-right.svg"
           top="/org.palladiosimulator.editors.sirius.assembly.custom/images/infrastructure-required-top.svg">
     </image>
  </extension>
  <extension
        id="org.palladiosimulator.editors.sirius.assembly.custom.sourceRole"
        point="org.palladiosimulator.editors.sirius.custom.style.rotatableNode">
     <image
           bottom="/org.palladiosimulator.editors.sirius.assembly.custom/images/source-role-bottom.svg"
           id="org.palladiosimulator.editors.sirius.assembly.custom.sourceRole"
           left="/org.palladiosimulator.editors.sirius.assembly.custom/images/source-role-left.svg"
           right="/org.palladiosimulator.editors.sirius.assembly.custom/images/source-role-right.svg"
           top="/org.palladiosimulator.editors.sirius.assembly.custom/images/source-role-top.svg">
     </image>
  </extension>
  <extension
        id="org.palladiosimulator.editors.sirius.assembly.custom.sinkRole"
        point="org.palladiosimulator.editors.sirius.custom.style.rotatableNode">
     <image
           bottom="/org.palladiosimulator.editors.sirius.assembly.custom/images/sink-role-bottom.svg"
           id="org.palladiosimulator.editors.sirius.assembly.custom.sinkRole"
           left="/org.palladiosimulator.editors.sirius.assembly.custom/images/sink-role-left.svg"
           right="/org.palladiosimulator.editors.sirius.assembly.custom/images/sink-role-right.svg"
           top="/org.palladiosimulator.editors.sirius.assembly.custom/images/sink-role-top.svg">
     </image>
  </extension>
</plugin>
